{
  "plugins": [
    "@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator",
    "@babel/plugin-proposal-function-bind",
    [
      "wildcard",
      {
        "noModifyCase": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": { "styles": "./styles" },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": []
}